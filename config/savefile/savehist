;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by `savehist'.

(setq savehist-minibuffer-history-variables '(extended-command-history ido-file-history))
(setq extended-command-history '("make-directory"))
(setq ido-file-history '("save-file"))
(setq regexp-search-ring 'nil)
